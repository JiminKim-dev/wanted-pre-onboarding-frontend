{
  "compilerOptions": {
    "target": "es2016",
    "lib": ["ES6", "DOM"],
    "allowJs": true,
    "jsx": "react-jsx",
    "module": "commonjs",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "noEmit": true
  },
  "include": ["src", "craco.config.js"],
  "extends": "./tsconfig.paths.json"
}
